<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.carwash.carwash.activities.SearchResultsActivity">

    <TableLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:id="@+id/test1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorDescription"
                android:text="Name  "
                />
            <TextView
                android:id="@+id/resName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView"
                android:width="120dp"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:id="@+id/test2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorDescription"
                android:text="Surname "
                />
            <TextView
                android:id="@+id/resSurname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView"
                android:width="120dp"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            >
            <TextView
                android:id="@+id/test3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorDescription"
                android:text="Date Of Birth  "
                />
            <TextView
                android:id="@+id/resDateOfBirth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView"
                android:width="320dp"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/test4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorDescription"
                android:text="Cell Number" />
            <TextView
                android:id="@+id/resCellNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/test5"
                android:layout_width="wrap_content"
                android:background="@color/colorDescription"
                android:layout_height="wrap_content"
                android:text="Registration No" />
            <TextView
                android:id="@+id/resRegNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView" />

        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/test6"
                android:layout_width="wrap_content"
                android:background="@color/colorDescription"
                android:layout_height="wrap_content"
                android:text="Created Date" />

            <TextView
                android:id="@+id/resModified"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/test7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorDescription"
                android:text="Wash count" />

            <TextView
                android:id="@+id/resWashCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorValue"
                android:text="TextView" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <Button
                android:id="@+id/homeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="home"
                android:text="Home" />
            <Button
                android:id="@+id/washButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="washCar"
                android:text="Wash Car" />

        </TableRow>
    </TableLayout>
</android.support.constraint.ConstraintLayout>
